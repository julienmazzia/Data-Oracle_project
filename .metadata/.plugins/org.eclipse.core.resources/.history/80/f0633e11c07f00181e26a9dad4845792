package Model;

public class OrderCreator {
	public static void createOrders(int nombre, String type){
		double marge_erreur;
		switch(type){
			case "faible":
				marge_erreur = 0.01;
				break;
			case "moyen":
				marge_erreur = 0.05;
				break;
			case "fort":
				marge_erreur = 0.1;
				break;
		}
		Order order;
		for(int i = 0; i<nombre; i++){
			order = Order.create(0, 0);
		}
	}
}
