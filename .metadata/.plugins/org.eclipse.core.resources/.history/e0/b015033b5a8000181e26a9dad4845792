package Model;

import java.util.ArrayList;
import java.util.List;

public class OrderCreator {
	public static Order[] createOrders(int number){
		List<Order> ListOrder = new ArrayList<Order>();
		Order[] tabOrder= {};
		Order order;
		for(int i = 0; i<number; i++){
			order = Order.create();
			ListOrder.add(order);
		}
		return tabOrder;
	}
}
